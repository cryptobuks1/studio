{
    "name": "cnvs/studio",
    "description": "Medium-inspired UI for Canvas.",
    "keywords": ["canvas", "laravel", "studio", "ui"],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/console": "^6.0",
        "illuminate/filesystem": "^6.0",
        "illuminate/support": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Studio\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Studio\\StudioServiceProvider"
            ]
        }
    }
}
